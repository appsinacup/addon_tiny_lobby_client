{
  "name": "Tiny Lobby Client Dev",
  "image": "ghcr.io/appsinacup/addon_tiny_lobby_client-dev:main",
  "runArgs": [
    "-e", "PLATFORM=linux",
    "-e", "TARGET=template_debug"
  ],
  "mounts": [
